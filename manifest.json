{
  "manifest_version": 2,

  "name": "AeroWeather",
  "description": "Gives you the most recent METAR report for a given airport",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "https://aviationweather.gov/metar"
   ]
}
